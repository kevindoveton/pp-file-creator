extends _baseModal

block content
  h1 New User
  form(ng-submit="submit()")
    .inputField.black
      input.text-input(type="text" ng-model="user.username")
      label.active Username
    
    .inputField.black
      input.text-input(type="password" ng-model="user.password")
      label.active Password
      
    input.btn(type="submit")
